  "keywords": ["yii2", "ulogin", "oauth", "authorization"],
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://github.com/rmrevin/yii2-ulogin/issues?state=open",
    "wiki": "https://github.com/rmrevin/yii2-ulogin/wiki",
    "source": "https://github.com/rmrevin/yii2-ulogin"
  },
  "authors": [
    {
      "name": "Revin Roman",
      "email": "xgismox@gmail.com",
      "homepage": "http://phptime.ru/"
    },
    {
      "name": "uLogin",
      "homepage": "https://ulogin.ru/",
      "role": "OAuth provider"
    }
  ],
  "require": {
    "yiisoft/yii2": "*"
  },
  "autoload": {
    "psr-4": {
      "rmrevin\\yii\\ulogin\\": ""
